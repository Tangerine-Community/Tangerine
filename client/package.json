{
  "name": "tangerine",
  "version": "4.0.0",
  "description": "data collection system.",
  "main": "index.js",
  "scripts": {
    "postinstall": " cd scripts && ./postinstall.sh",
    "build:apk": "cordova build android 2>&1",
    "test": "grunt test",
    "clean": "rm -rf platforms plugins node_modules",
    "update:tree": "git pull && chown -R www-data:www-data .",
    "listen": "cd scripts ./listen.rb",
    "testW": "http-server -p 9000 --silent & ./scripts/listen.rb  & grunt coffeeW",
    "testWatch": "http-server -p 9000 --silent & grunt testWatch",
    "testInBrowser": "http-server ./ -p 9000 --silent",
    "install:apk": "cd scripts && ./install.sh",
    "preload": "scripts/preload.js",
    "start": "gulp",
    "gulp": "gulp",
    "debug": "gulp index-dev & gulp default",
    "debug-test": "gulp debug & http-server ./ -p 9000 --silent",
    "debug-gulp": "iron-node node_modules/gulp/bin/gulp.js"
  },
  "repository": {
