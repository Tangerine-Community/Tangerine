{
  "name": "tangerine-form",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"./node_modules/.bin/angular-pages build --watch\" \"ng serve\"",
    "build": "./node_modules/.bin/angular-pages build && ng build --aot --prod && sw-precache --verbose --config=sw-precache-config.js",
    "test": "./node_modules/.bin/angular-pages build && ng test",
    "lint": "./node_modules/.bin/angular-pages build && ng lint",
    "e2e": "./node_modules/.bin/angular-pages build && ng e2e",
    "pages": "./node_modules/.bin/angular-pages build --watch",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "lite-server": "cd lite-server && node app.js",
    "publish": "npm run build && git clone git@github.com:tangerine-v3-demo/tangerine-v3-demo.github.io .publish-tmp && mv .publish-tmp/.git dist/ && rm -r .publish-tmp && cd dist && git add . && git commit -m \"npm publish\" && git push && cd .."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.28",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/store": "^2.2.2",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/js-yaml": "^3.5.31",
    "angular-pages": "^0.1.1",
    "angular-tree-component": "^3.7.2",
    "angular2-uuid": "^1.1.1",
    "async-child-process": "^1.1.1",
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "csv-file-creator": "^1.0.7",
    "express-pouchdb": "^2.3.7",
    "gh-markdown-cli": "^0.2.0",
    "hammerjs": "^2.0.8",
    "js-yaml": "^3.8.4",
    "ng-formly": "^1.0.0-rc.9",
    "pouchdb": "^6.2.0",
    "read-yaml": "^1.1.0",
    "rxjs": "^5.1.0",
    "watch": "^1.0.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "sw-precache": "^5.2.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
