{
  "name": "tangerine",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "bin": {
    "info": "./src/scripts/info.sh",
    "reporting-cache-clear": "./src/scripts/reporting-cache-clear.js",
    "reporting-worker-batch": "./src/scripts/reporting-worker-batch.js",
    "reporting-worker-unpause": "./src/scripts/reporting-worker-unpause.sh",
    "reporting-worker-pause": "./src/scripts/reporting-worker-pause.sh",
    "generate-csv": "./src/scripts/generate-csv/bin.js",
    "generate-uploads": "./src/scripts/generate-uploads/bin.js",
    "push-all-groups-views": "./src/scripts/push-all-groups-views.js",
    "find-missing-records": "./src/scripts/find-missing-records.js",
    "import-archives": "./src/scripts/import-archives/bin.js",
    "translations-update": "./src/scripts/translations-update.js",
    "release-dat": "./src/scripts/release-dat.sh",
    "release-apk": "./src/scripts/release-apk.sh",
    "release-pwa": "./src/scripts/release-pwa.sh",
    "import-v2-assessment": "./src/scripts/import-v2-assessment.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "comma-separated-values": "^3.6.4",
    "compression": "^1.7.2",
    "cordova-hot-code-push-cli": "^1.1.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "exceljs": "^1.2.1",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "express-session": "1.15.6",
    "flat": "^4.0.0",
    "fs-extra": "^4.0.3",
    "json2csv": "^3.11.5",
    "junk": "^2.1.0",
    "lodash": "^4.17.10",
    "multer": "^1.4.1",
    "pako": "^1.0.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pouchdb": "^6.4.3",
    "pouchdb-find": "^6.4.3",
    "pouchdb-upsert": "^2.2.0",
    "pretty": "^2.0.0",
    "read-yaml": "^1.1.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "sanitize-filename": "^1.6.1",
    "session-pouchdb-store": "0.2.1",
    "tangy-log": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "@nestjs/testing": "^6.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
